{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React.js Default",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "allowImportingTsExtensions": true
  }
}
