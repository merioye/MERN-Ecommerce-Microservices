{
  "name": "@ecohatch/utils-api",
  "version": "1.0.0",
  "description": "API utils for ecohatch internal applications/packages",
  "author": "Umair Saleem",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "build": "nest build",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:band": "jest --runInBand --passWithNoTests",
    "test:watchBand": "jest --watchAll --runInBand --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "clean": "node scripts/clean.mjs"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "class-transformer": "^0.5.1",
    "nestjs-i18n": "^10.4.5",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@ecohatch/config-eslint": "workspace:*",
    "@ecohatch/config-jest": "workspace:*",
    "@ecohatch/config-prettier": "workspace:*",
    "@ecohatch/config-typescript": "workspace:*",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.8.3",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.56.0",
    "jest": "^29.5.0",
    "prettier": "^3.3.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
