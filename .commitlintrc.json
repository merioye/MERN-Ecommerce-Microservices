{
  "extends": ["@commitlint/config-conventional"],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "ci",
        "chore",
        "docs",
        "feat",
        "fix",
        "perf",
        "refactor",
        "revert",
        "style",
        "assets",
        "test",
        "build",
        "deps",
        "config",
        "env",
        "infra",
        "lint",
        "data",
        "ui",
        "util"
      ]
    ]
  }
}
