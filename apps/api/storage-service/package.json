{
  "name": "@ecohatch/storage-service",
  "version": "1.0.0",
  "description": "A production-ready microservice for handling secure file uploads and deletions, providing secure and efficient storage management.",
  "author": "Umair Saleem",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "build": "nest build",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:debug": "cross-env NODE_ENV=debuging nest start --debug --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:band": "jest --runInBand --passWithNoTests",
    "test:watchBand": "jest --watchAll --runInBand --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.js --passWithNoTests",
    "test:e2e-watch": "jest --config jest.e2e.config.js --watch --passWithNoTests",
    "test:e2e-band": "jest --config jest.e2e.config.js --runInBand --passWithNoTests",
    "test:e2e-watchBand": "jest --config jest.e2e.config.js --watchAll --runInBand --passWithNoTests",
    "test:e2e:ci": "jest --config jest.e2e.config.js --ci --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "clean": "node scripts/clean.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/s3-request-presigner": "^3.744.0",
    "@ecohatch/utils-api": "workspace:*",
    "@ecohatch/utils-shared": "workspace:*",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/swagger": "^7.1.14",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.0.0",
    "http-terminator": "^3.2.0",
    "joi": "^17.11.0",
    "mongoose": "^8.10.1",
    "nestjs-graceful-shutdown": "^1.0.3",
    "nestjs-i18n": "^10.4.5",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@ecohatch/config-eslint": "workspace:*",
    "@ecohatch/config-jest": "workspace:*",
    "@ecohatch/config-prettier": "workspace:*",
    "@ecohatch/config-typescript": "workspace:*",
    "@ecohatch/types-shared": "workspace:*",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.3",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.5.0",
    "prettier": "^3.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
